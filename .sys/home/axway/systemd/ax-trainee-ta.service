[Unit]
Description= [Trainee] Enterprise Edge Gateway Traceability Agent
Requires=network-online.target
After=network-online.target

[Service]
WatchdogSec=600
ExecStart=/opt/Axway/agents/trainee/traceability_agent --path.config /opt/Axway/agents/trainee --envFile /opt/Axway/agents/trainee/ta_env_vars.env
Restart=always
User=axway
Group=axway

[Install]
WantedBy=multi-user.target
